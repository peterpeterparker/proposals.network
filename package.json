{
	"name": "proposals.network",
	"version": "0.0.14",
	"private": true,
	"license": "MIT",
	"author": "David Dal Busco (https://daviddalbusco.com)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peterpeterparker/proposals.network.git"
	},
	"bugs": {
		"url": "https://github.com/peterpeterparker/proposals.network"
	},
	"type": "module",
	"scripts": {
		"build:csp": "node scripts/build.csp.mjs",
		"build:post-process": "npm run build:csp",
		"build:snses": "node scripts/build.snses.mjs && prettier --write src/lib/data/snses.json",
		"build": "npm run build:snses && tsc --noEmit && vite build && npm run build:post-process",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check './**/*.{ts,js,mjs,json,scss,css,svelte,html,md}' && eslint . --max-warnings=0",
		"format": "prettier --write './**/*.{ts,js,mjs,json,scss,css,svelte,html,md}'",
		"e2e": "playwright test",
		"e2e:ci": "playwright test --reporter=html",
		"e2e:snapshots": "playwright test --update-snapshots --reporter=list",
		"e2e:report": "playwright show-report",
		"dev": "vite dev",
		"dev:prod": "NODE_ENV=production JUNO_ANALYTICS=disabled vite dev --mode production",
		"deploy": "./junolator deploy",
		"config": "./junolator config",
		"collections": "node scripts/collections.mjs",
		"postinstall_auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall": "npm run postinstall_auth"
	},
	"devDependencies": {
		"@dfinity/identity-secp256k1": "^3.2.6",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@junobuild/cli-tools": "^0.7.2",
		"@junobuild/config": "^2.1.1",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.8",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.1.13",
		"@types/dom-view-transitions": "^1.0.6",
		"@types/node": "^22.15.3",
		"autoprefixer": "^10.4.21",
		"dotenv": "^16.5.0",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
		"pem-file": "^1.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.87.0",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.7",
		"tailwindcss": "^4.1.13",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.31.1",
		"vite": "^6.3.5",
		"vitest": "^3.1.3"
	},
	"dependencies": {
		"@dfinity/identity": "^3.2.6",
		"@dfinity/ledger-icp": "^6.0.0",
		"@dfinity/ledger-icrc": "^4.0.1",
		"@dfinity/nns": "^10.0.1",
		"@dfinity/principal": "^3.2.6",
		"@dfinity/sns": "^4.0.2",
		"@dfinity/utils": "^3.1.0",
		"@junobuild/admin": "^2.2.2",
		"@junobuild/analytics": "^0.2.7",
		"@junobuild/core": "^2.2.0",
		"@tiptap/core": "^2.12.0",
		"@tiptap/extension-code-block-lowlight": "^2.12.0",
		"@tiptap/extension-highlight": "^2.12.0",
		"@tiptap/extension-link": "^2.12.0",
		"@tiptap/extension-table": "^2.12.0",
		"@tiptap/extension-table-cell": "^2.12.0",
		"@tiptap/extension-table-header": "^2.12.0",
		"@tiptap/extension-table-row": "^2.12.0",
		"@tiptap/extension-typography": "^2.12.0",
		"@tiptap/extension-underline": "^2.12.0",
		"@tiptap/pm": "^2.12.0",
		"@tiptap/starter-kit": "^2.12.0",
		"dompurify": "^3.2.5",
		"idb-keyval": "^6.2.1",
		"lowlight": "^3.3.0",
		"marked": "^15.0.11",
		"nanoid": "^5.1.5",
		"svelte-confetti": "^2.3.1",
		"tiptap-markdown": "^0.8.10",
		"yaml": "^2.7.1",
		"zod": "^3.25.71"
	}
}
