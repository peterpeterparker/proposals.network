{
	"name": "proposals.network",
	"version": "0.0.9",
	"private": true,
	"license": "MIT",
	"author": "David Dal Busco (https://daviddalbusco.com)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peterpeterparker/proposals.network.git"
	},
	"bugs": {
		"url": "https://github.com/peterpeterparker/proposals.network"
	},
	"type": "module",
	"scripts": {
		"build:csp": "node scripts/build.csp.mjs",
		"build:post-process": "npm run build:csp",
		"build:snses": "node scripts/build.snses.mjs && prettier --write src/lib/data/snses.json",
		"build": "npm run build:snses && tsc --noEmit && vite build && npm run build:post-process",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check './**/*.{ts,js,mjs,json,scss,css,svelte,html,md}' && eslint . --max-warnings=0",
		"format": "prettier --write './**/*.{ts,js,mjs,json,scss,css,svelte,html,md}'",
		"e2e": "playwright test",
		"e2e:ci": "playwright test --reporter=html",
		"e2e:snapshots": "playwright test --update-snapshots --reporter=list",
		"e2e:report": "playwright show-report",
		"dev": "vite dev",
		"dev:prod": "NODE_ENV=production JUNO_ANALYTICS=disabled vite dev --mode production",
		"deploy": "./junolator deploy",
		"config": "./junolator config",
		"collections": "node scripts/collections.mjs",
		"postinstall_analytics": "node -e \"require('fs').cpSync('node_modules/@junobuild/analytics/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall_auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall": "npm run postinstall_auth && npm run postinstall_analytics"
	},
	"devDependencies": {
		"@dfinity/identity-secp256k1": "^2.2.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.19.0",
		"@junobuild/cli-tools": "^0.1.0",
		"@junobuild/config": "^0.1.0",
		"@playwright/test": "^1.50.0",
		"@rollup/plugin-inject": "^5.0.5",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.3",
		"@types/dom-view-transitions": "^1.0.5",
		"@types/node": "^22.12.0",
		"dotenv": "^16.4.7",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"pem-file": "^1.0.1",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.79.3",
		"svelte": "^5.19.4",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.1.3",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.22.0",
		"vite": "^6.0.11",
		"vitest": "^3.0.4"
	},
	"dependencies": {
		"@dfinity/identity": "^2.2.0",
		"@dfinity/ledger-icp": "^2.6.7",
		"@dfinity/ledger-icrc": "^2.7.2",
		"@dfinity/nns": "^8.2.1",
		"@dfinity/principal": "^2.2.0",
		"@dfinity/sns": "^3.2.8",
		"@dfinity/utils": "^2.9.0",
		"@junobuild/admin": "^0.1.3",
		"@junobuild/analytics": "^0.1.1",
		"@junobuild/core": "^0.1.4",
		"@tiptap/core": "^2.11.3",
		"@tiptap/extension-code-block-lowlight": "^2.11.3",
		"@tiptap/extension-highlight": "^2.11.3",
		"@tiptap/extension-link": "^2.11.3",
		"@tiptap/extension-table": "^2.11.3",
		"@tiptap/extension-table-cell": "^2.11.3",
		"@tiptap/extension-table-header": "^2.11.3",
		"@tiptap/extension-table-row": "^2.11.3",
		"@tiptap/extension-typography": "^2.11.3",
		"@tiptap/extension-underline": "^2.11.3",
		"@tiptap/pm": "^2.11.3",
		"@tiptap/starter-kit": "^2.11.3",
		"dompurify": "^3.2.3",
		"idb-keyval": "^6.2.1",
		"lowlight": "^3.3.0",
		"marked": "^15.0.6",
		"nanoid": "^5.0.9",
		"svelte-confetti": "^2.3.1",
		"tiptap-markdown": "^0.8.10",
		"yaml": "^2.7.0",
		"zod": "^3.24.1"
	}
}
