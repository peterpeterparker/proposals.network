{
	"name": "proposals.network",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"author": "David Dal Busco (https://daviddalbusco.com)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peterpeterparker/proposals.network.git"
	},
	"bugs": {
		"url": "https://github.com/peterpeterparker/proposals.network"
	},
	"type": "module",
	"scripts": {
		"build:csp": "node scripts/build.csp.mjs",
		"build:post-process": "npm run build:csp",
		"build:snses": "node scripts/build.snses.mjs",
		"build": "npm run build:snses && tsc --noEmit && vite build && npm run build:post-process",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check './**/*.{ts,js,mjs,json,scss,css,svelte,html,md}' && eslint . --max-warnings=0",
		"format": "prettier --write './**/*.{ts,js,mjs,json,scss,css,svelte,html,md}'",
		"e2e": "playwright test",
		"e2e:ci": "playwright test --reporter=html",
		"e2e:snapshots": "playwright test --update-snapshots --reporter=list",
		"e2e:report": "playwright show-report",
		"dev": "vite dev",
		"dev:prod": "NODE_ENV=production JUNO_ANALYTICS=disabled vite dev --mode production",
		"deploy": "./junolator deploy",
		"config": "./junolator config",
		"collections": "node scripts/collections.mjs",
		"postinstall_analytics": "node -e \"require('fs').cpSync('node_modules/@junobuild/analytics/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall_auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core-peer/dist/workers/', './static/workers', {recursive: true});\"",
		"postinstall": "npm run postinstall_auth && npm run postinstall_analytics"
	},
	"devDependencies": {
		"@dfinity/identity-secp256k1": "^0.20.2",
		"@junobuild/admin": "^0.0.40",
		"@junobuild/vite-plugin": "^0.0.5",
		"@playwright/test": "^1.40.1",
		"@rollup/plugin-inject": "^5.0.5",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/dom-view-transitions": "^1.0.4",
		"@types/dompurify": "^3.0.5",
		"@types/node": "^20.10.5",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.3.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"node-fetch": "^3.3.2",
		"pem-file": "^1.0.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.69.5",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.10",
		"vitest": "^1.1.0"
	},
	"dependencies": {
		"@dfinity/identity": "^0.20.2",
		"@dfinity/ledger-icp": "^2.1.0",
		"@dfinity/nns": "^3.1.0",
		"@dfinity/principal": "^0.20.2",
		"@dfinity/sns": "^2.1.0",
		"@dfinity/utils": "^2.0.0",
		"@junobuild/analytics": "^0.0.15",
		"@junobuild/core-peer": "^0.0.5",
		"@tiptap/core": "^2.1.13",
		"@tiptap/extension-code-block-lowlight": "^2.1.13",
		"@tiptap/extension-highlight": "^2.1.13",
		"@tiptap/extension-link": "^2.1.13",
		"@tiptap/extension-table": "^2.1.13",
		"@tiptap/extension-table-cell": "^2.1.13",
		"@tiptap/extension-table-header": "^2.1.13",
		"@tiptap/extension-table-row": "^2.1.13",
		"@tiptap/extension-typography": "^2.1.13",
		"@tiptap/extension-underline": "^2.1.13",
		"@tiptap/pm": "^2.1.13",
		"@tiptap/starter-kit": "^2.1.13",
		"dompurify": "^3.0.6",
		"idb-keyval": "^6.2.1",
		"lowlight": "^3.1.0",
		"marked": "^11.1.0",
		"nanoid": "^5.0.4",
		"svelte-confetti": "^1.3.1",
		"tiptap-markdown": "^0.8.8"
	}
}
